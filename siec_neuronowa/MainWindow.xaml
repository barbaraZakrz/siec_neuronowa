<Window x:Class="siec_neuronowa.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:siec_neuronowa"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="Struktura" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="wpisz strukturę" VerticalAlignment="Top" Width="120" Margin="81,83,0,0" TextChanged="Struktura_TextChanged"/>
        <Button x:Name="CreateBTN" Content="Stwórz" HorizontalAlignment="Left" Margin="206,82,0,0" VerticalAlignment="Top" Width="75" Click="CreateBTN_Click"/>
        <TextBox x:Name="Inputs" HorizontalAlignment="Left" Height="23" Margin="81,111,0,0" TextWrapping="Wrap" Text="Wpisz wejścia (ze spacjami)" VerticalAlignment="Top" Width="175" TextChanged="Inputs_TextChanged"/>
        <Button x:Name="RunBTN" Content="Przelicz" HorizontalAlignment="Left" Margin="261,111,0,0" VerticalAlignment="Top" Width="75" Click="RunBTN_Click"/>
        <TextBox x:Name="Outputs" HorizontalAlignment="Left" Height="23" Margin="341,111,0,0" TextWrapping="Wrap" Text="Wyniki" VerticalAlignment="Top" Width="120" TextChanged="Outputs_TextChanged"/>

    </Grid>
</Window>
